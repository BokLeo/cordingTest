{
	"compilerOptions": {
		"target": "es6",
		"lib": ["es2017", "dom", "es2019"],
		"module": "commonjs",
		"outDir": "./dist",
		"strict": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
